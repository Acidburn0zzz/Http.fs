    "version": "2.0.0",
    "tasks": [
        {
            "taskName": "quick build",
            "command": "dotnet build --no-restore",
            "type": "shell",
            "group": {
                "isDefault": true,
                "kind": "build"
            },
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        }
    ]
}